CREATE DEFINER=`root`@`localhost` PROCEDURE `tabuada`(vNUMERO INT)
BEGIN
DECLARE vRESULTADO INT(3);
DECLARE vFINAL INT;
SET vFINAL = 10*vNUMERO;
SET vRESULTADO = 0;

DELETE FROM `TAB_LOOPING`;
WHILE vRESULTADO <= vFINAL
DO
	INSERT INTO `TAB_LOOPING`(ID) VALUES (vRESULTADO);
    SET vRESULTADO = vRESULTADO + vNUMERO;
END WHILE;
SELECT * FROM TAB_LOOPING;
END
