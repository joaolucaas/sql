create table vendedores (
Nome varchar(50),
matricula varchar(7),
comissao float );

select * from vendedores;

insert into vendedores
(Nome, matricula, comissao) values
('Bariri','123.456',1.90);

insert into vendedores
(Nome, matricula, comissao) values
('Allan','123.321',1.80);

